  "scripts": {
    "migrations:generate": "tsx ./node_modules/typeorm/cli.js migration:generate -d ./src/data-source.ts src/migration/migration",
    "migrations:run": "tsx ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts",
    "build": "npx tsc --build --verbose && cp ./src/current-path.cjs ./dist/current-path.cjs",
    "test:unit": "NODE_ENV=test jest --watch --testPathPattern=src",
    "test:int": "APIKEY=dh2873hd8qwegiuf873wgf783w4 NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start:dev": "APIKEY=dev-key NODE_ENV=dev tsx watch src/api/main.ts & NODE_ENV=dev tsx watch src/content-server/main.ts",
    "start:content-server": "pm2 start src/content-server/ecosystem.content-server.config.cjs --no-daemon",
    "start:api": "pm2 start src/api/ecosystem.api.config.cjs --no-daemon",
    "start": "pm2 start src/content-server/ecosystem.content-server.config.cjs --no-daemon && pm2 start src/api/ecosystem.api.config.cjs --no-daemon"
  },
